server {
  listen              80 default_server;
  listen              [::]:80 default_server ;
  server_name         _;

  proxy_set_header   Host $host;
  proxy_set_header   X-Forwarded-Proto https;

  # Current Omejdn
  location  / {
    proxy_pass         http://omejdn-server:4567;
    proxy_redirect     off;
  }
}
